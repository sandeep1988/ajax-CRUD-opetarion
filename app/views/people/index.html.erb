<p id="notice"><%= notice %></p>

<h1>Listing People</h1>

<table class='table' id='people_table'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 <tbody id="peoples">
    <% @people.each do |person| %>
      <%= render partial: 'table_row', locals: {person: person} %>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Person', new_person_path, remote: true, id: "new_person_id" %>
<div class="new-person"></div>
<div class="new_person1"></div>

